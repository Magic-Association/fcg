{
  "compilerOptions": {
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "target": "ESNext",
	"lib": ["ESNext"],
    "sourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
	"strict": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["src"]
}
