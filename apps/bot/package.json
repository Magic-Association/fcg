{
  "name": "fcg-bot",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=24.11.0"
  },
  "scripts": {
    "dev": "tsx watch ./src/index.ts",
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "node --enable-source-maps ./dist/index.js",
    "register": "tsx ./src/scripts/registerCommands.ts",
    "lint": "eslint",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "discord.js": "^14.23.2",
    "tsx": "^4.20.6"
  },
  "devDependencies": {
    "tsc-alias": "^1.8.10",
    "typescript": "^5.9.3"
  }
}
